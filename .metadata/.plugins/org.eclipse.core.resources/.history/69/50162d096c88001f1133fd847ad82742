package test;

import static org.junit.Assert.*;
import solu.*;

public class Test {

	private Solution4 solu =new Solution4();
	int[] num1= {3,6,9,1};
	int[] num2= {10};
	
	@org.junit.Test
	public void test() {
		assertEquals(3,solu.maximumGap(num1));
		assertEquals(0,solu.maximumGap(num2));
	}

}
